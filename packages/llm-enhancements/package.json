{
	"name": "@nuasite/llm-enhancements",
	"description": "Expose pages as .md endpoints for Astro",
	"files": [
		"dist/**",
		"src/**",
		"README.md",
		"package.json"
	],
	"homepage": "https://github.com/nuasite/nuasite/blob/main/packages/llm-enhancements/README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nuasite/nuasite.git",
		"directory": "packages/llm-enhancements"
	},
	"license": "Apache-2.0",
	"version": "0.9.2",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts",
			"default": "./src/index.ts"
		}
	},
	"dependencies": {
		"astro": "catalog:astro",
		"node-html-parser": "catalog:parsers"
	},
	"devDependencies": {
		"@types/bun": "catalog:build"
	},
	"peerDependencies": {
		"typescript": "catalog:build",
		"vite": "catalog:build",
		"@nuasite/cms": "workspace:*"
	},
	"peerDependenciesMeta": {
		"@nuasite/cms": {
			"optional": true
		}
	},
	"scripts": {
		"prepack": "bun run ../../scripts/workspace-deps/resolve-deps.ts"
	},
	"keywords": [
		"markdown",
		"astro",
		"nuasite",
		"withastro",
		"pages"
	]
}

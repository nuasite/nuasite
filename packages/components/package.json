{
	"name": "@nuasite/components",
	"description": "Nua Site astro components.",
	"license": "Apache-2.0",
	"version": "0.0.100",
	"files": [
		"dist/**",
		"src/**",
		"README.md",
		"package.json"
	],
	"homepage": "https://github.com/nuasite/nuasite/blob/main/packages/components/README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nuasite/nuasite.git",
		"directory": "packages/components"
	},
	"scripts": {
		"prepack": "bun run ../../scripts/workspace-deps/resolve-deps.ts"
	},
	"type": "module",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts",
			"default": "./src/index.ts"
		},
		"./Form.astro": {
			"types": "./src/form/index.astro",
			"import": "./src/form/index.astro",
			"default": "./src/form/index.astro"
		},
		"./Image.astro": {
			"types": "./src/image/index.astro",
			"import": "./src/image/index.astro",
			"default": "./src/image/index.astro"
		}
	},
	"peerDependencies": {
		"typescript": "catalog:build"
	},
	"keywords": [
		"nuasite",
		"ui",
		"withastro"
	]
}
